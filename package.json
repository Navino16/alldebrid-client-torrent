  "scripts": {
    "build": "rm -rf build/ && npx tsc",
    "build:dev": "rm -rf build/ && npx tsc --pretty",
    "build:watch": "rm -rf build/ && npx tsc --pretty -w",
    "start": "node ./build/app.js",
    "start:watch": "nodemon ./build/app.js",
    "lint": "eslint src/",
    "test": "echo \"Error: no test specified\" && exit 1"
